# 청소년 진로체험 플랫폼 환경 변수 예시 파일
# 이 파일은 깃허브에 포함시켜 템플릿으로 사용하세요.
# 실제 값은 .env.local 에서 관리하세요.

# 🔹 외부 서비스 URL
NEXT_PUBLIC_URL_CAREER_TEST=
NEXT_PUBLIC_URL_STUDENT_RECORD=
NEXT_PUBLIC_URL_MOCK_INTERVIEW=

# 🔹 AI / 번역 API 키 (예: Gemini, OpenAI, Papago 등)
NEXT_PUBLIC_AI_API_KEY=
NEXT_PUBLIC_TRANSLATE_API_KEY=

# 🔹 백엔드 서버 API 엔드포인트
NEXT_PUBLIC_API_BASE_URL=

# 🔹 기타 (필요 시 추가)
NEXT_PUBLIC_KAKAO_APP_KEY=
NEXT_PUBLIC_GOOGLE_CLIENT_ID=

# === Generative AI (OpenAI 등) ===
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
AI_MODEL=gpt-4o-mini
AI_BASE_URL=https://api.openai.com/v1


# === Kakao OAuth/REST ===
KAKAO_REST_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
KAKAO_REDIRECT_URI=http://localhost:3000/api/kakao/token/callback
KAKAO_ADMIN_KEY=KakaoAK xxxxxxxxxxxxxxxxxxxxxxxxxxxx
KAKAO_CLIENT_SECRET=

# === Kakao JavaScript SDK (프론트) ===
NEXT_PUBLIC_KAKAO_JAVASCRIPT_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxx
NEXT_PUBLIC_APP_BASE_URL=http://localhost:3000

# === Gemini (무료 API) 설정 ===
GEMINI_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxx
GEMINI_BASE_URL=https://api.gemini.example.com
  # 예시 URL
GEMINI_MODEL=gemini-1.5  # 모델 이름 (버전 등)